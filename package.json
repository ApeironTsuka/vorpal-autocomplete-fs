{
  "name": "@ApeironTsuka/vorpal-autocomplete-fs",
  "version": "0.1.0",
  "description": "File System autocompletion for Vorpal",
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "test": "mocha && xo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ApeironTsuka/vorpal-autocomplete-fs.git"
  },
  "keywords": [
    "vorpal",
    "extension",
    "autocompletion",
    "autocomplete",
    "tab",
    "tabbed completion",
    "fs",
    "file system",
    "cli",
    "immersive",
    "repl",
    "vorpal",
    "vorpal.js",
    "linux",
    "unix",
    "posix"
  ],
  "author": "ApeironTsuka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ApeironTsuka/vorpal-autocomplete-fs/issues"
  },
  "homepage": "https://github.com/ApeironTsuka/vorpal-autocomplete-fs#readme",
  "devDependencies": {
    "fs-extra": "^11.2.0",
    "lodash": "^4.17.21",
    "mocha": "^10.4.0",
    "shelljs": "^0.8.5",
    "should": "^13.2.3",
    "@ApeironTsuka/vorpal": "*",
    "xo": "^0.58.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "strip-ansi": "^7.1.0"
  },
  "engines": {
    "node": ">= 14.0.0"
  },
  "xo": {
    "envs": [
      "node",
      "mocha"
    ],
    "esnext": true,
    "space": true,
    "rules": {
      "no-unused-expressions": 0,
      "no-nested-ternary": 0,
      "no-unneeded-ternary": 0,
      "no-implicit-coercion": 0,
      "prefer-arrow-callback": 0,
      "prefer-reflect": 0
    }
  }
